<% var pages = [ { pageName: 'About', pageDir: 'about' }, { pageName: 'Product', pageDir: 'product' }, { pageName:
'News', pageDir: 'news' }, { pageName: 'Blog', pageDir: 'blog' }, { pageName: 'Contact', pageDir: 'contact' } ] %>

<header class="l-header" role="banner">
  <div class="l-header__inner">
    <h1 class="l-header__logo">
      <a class="l-header__logoLink">ロゴ</a>
    </h1>
    <nav id="gnav" class="l-header__gnav" role="navigation">
      <ul class="l-header__gnavList" role="list">
        <% for (var i in pages) { %>
        <li class="l-header__gnavListItem" role="listitem">
          <a class="l-header__gnavListLink" href="/<%- pages[i].pageDir %>/"><%- pages[i].pageName %></a>
        </li>
        <% } %>
      </ul>
    </nav>

    <button id="menu-trigger" class="l-header__trigger">
      <span class="l-header__triggerItem"></span>
      <span class="l-header__triggerItem"></span>
      <span class="l-header__triggerItem"></span>
    </button>
  </div>
</header>
